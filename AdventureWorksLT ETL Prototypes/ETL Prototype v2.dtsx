                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
  T1.[SalesOrderID]  
, [SalesOrderDetailID] 
, T3.[CustomerKey]
, T4.[ProductKey]
, [OrderDateKey] = T5.CalendarDateKey
, [ShipDateKey] = T6.CalendarDateKey
, [OrderQty]
, [UnitPrice]
, [UnitPriceDiscount]
FROM [AdventureWorksLT2012].[SalesLT].[SalesOrderDetail] as T1
JOIN [AdventureWorksLT2012].[SalesLT].[SalesOrderHeader] as T2
	ON T1.[SalesOrderID] = T2.[SalesOrderID]
JOIN [DWAdventureWorksLT2012].[dbo].[DimCustomers] as T3
	ON T2.[CustomerID] = T3.[CustomerID]
JOIN [DWAdventureWorksLT2012].[dbo].[DimProducts] as T4
	ON T4.[ProductID] = T1.[ProductID]
JOIN [DWAdventureWorksLT2012].[dbo].[DimDates] as T5
	ON Cast(T5.CalendarDate as Date) = Cast(T2.[OrderDate] as Date)
JOIN [DWAdventureWorksLT2012].[dbo].[DimDates] as T6
	ON Cast(T6.CalendarDate as Date) = Cast(T2.[ShipDate] as Date)</property>
